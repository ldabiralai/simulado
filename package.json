{
  "name": "simulado",
  "version": "3.2.2",
  "description": "A simple nodejs mockserver",
  "main": "src/index.js",
  "bin": {
    "simulado": "bin/simulado"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldabiralai/simulado.git"
  },
  "keywords": [
    "mock",
    "mockserver",
    "http"
  ],
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./bin/simulado",
    "watch": "nodemon -e js,html ./node_modules/.bin/babel-node ./bin/simulado",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill --require test.setup.js --recursive './src/**/*.test.js'",
    "compile": "./node_modules/.bin/babel src --out-dir lib",
    "format": "prettier --write './src/**/*.js' './bin/simulado'",
    "danger": "danger",
    "build:ui": "webpack --config ./src/ui/webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "css-loader": "^1.0.0",
    "danger": "^2.1.10",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "portscanner": "^2.2.0",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.23.0",
    "supertest": "^3.3.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "commander": "^2.18.0",
    "cors": "^2.7.1",
    "deep-equal": "^1.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "lodash.merge": "^4.6.1",
    "query-string": "^6.2.0",
    "spdy": "^3.4.7"
  }
}
