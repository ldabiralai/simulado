{
  "name": "simulado",
  "version": "3.2.2",
  "description": "A simple nodejs mockserver",
  "main": "src/index.js",
  "bin": {
    "simulado": "bin/simulado"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldabiralai/simulado.git"
  },
  "keywords": [
    "mock",
    "mockserver",
    "http"
  ],
  "scripts": {
    "start": "babel-node ./bin/simulado",
    "watch": "nodemon -e js,html node_modules/.bin/babel-node ./bin/simulado",
    "test": "mocha --compilers js:@babel/register --require test.setup.js --recursive './src/**/*.test.js'",
    "compile": "babel src --out-dir lib",
    "format": "prettier --write './src/**/*.js' './bin/simulado'",
    "danger": "danger"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "chai": "^4.2.0",
    "danger": "^2.1.10",
    "glob": "^7.1.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "portscanner": "^2.2.0",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "commander": "^2.18.0",
    "cors": "^2.7.1",
    "deep-equal": "^1.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "lodash.merge": "^4.6.1",
    "query-string": "^6.2.0",
    "spdy": "^3.4.7"
  }
}
